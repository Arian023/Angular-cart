#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/arian/dev/Angular-cart/node_modules/.pnpm/protractor@7.0.0/node_modules/protractor/bin/node_modules:/home/arian/dev/Angular-cart/node_modules/.pnpm/protractor@7.0.0/node_modules/protractor/node_modules:/home/arian/dev/Angular-cart/node_modules/.pnpm/protractor@7.0.0/node_modules:/home/arian/dev/Angular-cart/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/arian/dev/Angular-cart/node_modules/.pnpm/protractor@7.0.0/node_modules/protractor/bin/node_modules:/home/arian/dev/Angular-cart/node_modules/.pnpm/protractor@7.0.0/node_modules/protractor/node_modules:/home/arian/dev/Angular-cart/node_modules/.pnpm/protractor@7.0.0/node_modules:/home/arian/dev/Angular-cart/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../protractor/bin/protractor" "$@"
else
  exec node  "$basedir/../protractor/bin/protractor" "$@"
fi
